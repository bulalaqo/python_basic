#< sqlite >
import sqlite3 as db
conn = db.connect (dbfile)
cursor = conn.cursor() # 加载全部DB或者ARC数据为DataFrame文件
sql2 = 'select * from realtimedata' # 'realtimedata' 数据库表名               
df = pd.read_sql (sql2, conn)
conn.close() # 关闭数据库



